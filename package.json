{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "yourls",
  "title": "Yourls",
  "description": "Yourls Link Shortener",
  "icon": "command-icon.png",
  "author": "firhan_ramadhan",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show List",
      "description": "Template for showing a static list",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
